{
  "name": "@artbc-ui/react",
  "version": "1.0.13",
  "description": "React Artbc UI System",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebase": "postcss --config src/plugins/base src/plugins/base/*.css --base src/plugins --dir dist/plugins",
    "base": "cat dist/plugins/base/*.css > dist/plugins/base.css",
    "postbase": "prejss-cli dist/plugins/base.css --format commonjs",
    "preutilities:global": "postcss --config src/plugins/utilities/global src/plugins/utilities/global/*.css --base src/plugins --dir dist/plugins",
    "utilities:global": "cat dist/plugins/utilities/global/*.css > dist/plugins/utilities.css",
    "postutilities:global": "prejss-cli dist/plugins/utilities.css --format commonjs",
    "preutilities:unstyled": "postcss --config src/plugins/utilities/unstyled src/plugins/utilities/unstyled/*.css --base src/plugins --dir dist/plugins",
    "utilities:unstyled": "cat dist/plugins/utilities/unstyled/*.css > dist/plugins/utilities-unstyled.css",
    "postutilities:unstyled": "prejss-cli dist/plugins/utilities-unstyled.css --format commonjs",
    "preutilities:styled": "postcss --config src/plugins/utilities/styled src/plugins/utilities/styled/*.css --base src/plugins --dir dist/plugins",
    "utilities:styled": "cat dist/plugins/utilities/styled/*.css > dist/plugins/utilities-styled.css",
    "postutilities:styled": "prejss-cli dist/plugins/utilities-styled.css --format commonjs",
    "components": "postcss --config src/plugins/components src/plugins/components/**/*.css --base src/plugins --dir dist/plugins",
    "merge:unstyled": "cat dist/plugins/components/unstyled/*.css > dist/plugins/unstyled.css",
    "merge:styled": "cat dist/plugins/components/unstyled/*.css dist/plugins/components/styled/*.css > dist/plugins/styled.css",
    "prejss": "prejss-cli dist/plugins/{unstyled,styled}{,.rtl}.css --format commonjs",
    "themes": "postcss src/plugins/themes/index.css -o dist/plugins/themes.css --config src/plugins/themes",
    "full": "postcss src/plugins/full/index.css -o dist/plugins/full.css --config src/plugins/full",
    "build:plugins": "npm run base && npm run utilities:global && npm run utilities:unstyled && npm run utilities:styled && npm run components && npm run merge:unstyled && npm run merge:styled && npm run prejss && npm run themes && npm run full",
    "rollup-watch": "rollup -c -w",
    "build": "rollup -c",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artbycode-id/artbc-ui.git"
  },
  "keywords": [
    "UI"
  ],
  "author": "undercode99 <zrcoder101@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artbycode-id/artbc-ui/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack4": "^6.5.12",
    "@storybook/manager-webpack4": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "0.0.13",
    "@types/color": "^3.0.3",
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.21",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "postcss-nested": "^6.0.0",
    "prejss-cli": "^0.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "homepage": "https://artbycode-id.github.io/artbc-ui/",
  "dependencies": {
    "classnames": "^2.3.2",
    "color": "^4.2.3",
    "css-selector-tokenizer": "^0.8.0"
  }
}
